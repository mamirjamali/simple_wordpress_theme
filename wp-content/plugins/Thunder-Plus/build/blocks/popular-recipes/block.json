{
  "$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
  "apiVersion": 2,
  "name": "thunder-plus/popular-recipes",
  "title": "Popular Recipes",
  "category": "text",
  "description": "Adds a list of highly rated recipes",
  "textdomain": "thunder-plus",
  "attributes": {
    "title": {
      "type": "string",
      "default": "Popular Recipes"
    },
    "count": {
      "type": "number",
      "default": 3
    },
    "cuisines": {
      "type": "array",
      "default": []
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./index.css"
}